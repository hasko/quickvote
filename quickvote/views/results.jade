extends layout

block content
  h1 QuickVote Results
  p The current vote question is:
  p.lead(class="text-primary")= question
  ul
    each count, ans in tally
      li #{ans}: #{count}
  
  svg.chart
  script(src='https://d3js.org/d3.v3.min.js', charset="utf-8")
  script(src="/javascripts/barchart.js")

  p.info This vote expires in #{ttl} seconds.
